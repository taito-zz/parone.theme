    id="++resource++parone.theme.stylesheets/wedding.css"
    media="screen" rel="stylesheet" rendering="import"
    cacheable="True" compression="safe" cookable="True"
    enabled="1" expression="python: ('/weddings' in object_url) | ('/haat' in object_url)"/>

